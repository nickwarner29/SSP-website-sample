//getData().then(data => {
runIt(data => {
    const chart = Highcharts.chart('container', {
        chart: {
            type: 'scatter',
            zooming: {
                type: 'xy'
            }
        },
        title: {
            text: 'SSP-Sample',
            align: 'left'
        },
        subtitle: {
            text:
          'Source: <a href="https://www.elementhuman.com/">Element Human</a>',
            align: 'left'
        },
        xAxis: {
            title: {
                text: 'Staying Power'
            },
            labels: {
                format: '{value}'
            },
            lineWidth: 0,
            gridLineWidth: 1,
            tickWidth: 0,
            min: 0,
            max: 100,
            startOnTick: false,
            endOnTick: false,
            showLastLabel: true,
            visible: true
        },
        yAxis: {
            title: {
                text: 'Stopping Power'
            },
            labels: {
                format: '{value}'
            },
            min: 0,
            max: 100,
            showLastLabel: true
        },
        legend: {
            enabled: true
        },
        plotOptions: {
            scatter: {
                marker: {
                    radius: 5,
                    symbol: 'circle',
                    states: {
                        hover: {
                            enabled: true,
                            lineColor: 'rgb(100,100,100)'
                        }
                    }
                },
                states: {
                    hover: {
                        marker: {
                            enabled: false
                        }
                    }
                },
                jitter: {
                    x: 0.005
                }
            }
        },
        tooltip: {
            pointFormat: 'Creator: {point.custom.Creator}<br/>Brand Category: {point.custom.Brand Category}<br/>Brand Name: {point.custom.Brand Name}<br/>Platform: {point.custom.Platform}<br/> Staying Power: {point.x}<br/> Stopping Power: {point.y}'
        },
        series
    });

    const reload = () => {
        const checkFilters = (elm) => {
            let check = true;
            // check = ((filters[0].value !== null && value === filters[0].value) || filters[0].value === null);
            filters.forEach(f => check &&= ((f.value !== null && f.value === elm[f.id]) || f.value === null));
            return check;
        }

        const getData = Platform => {
            const temp = [];
            data.forEach(elm => {
                if (elm.Platform === Platform && checkFilters(elm))
                {
                    temp.push({
                        id: elm.Creator,
                        x: elm["Staying Power"],
                        y: elm["Stopping Power"],
                        custom: elm                  
                    });
                }
            });
            return temp;
        };

        series.forEach((s, index) => {
            const newData = getData(s.id);
            chart.series[index].setData(newData, false, true, true);
        });

        chart.redraw();
    };

    const getFilterData = filter => {
        const temp = [];
        data.forEach(elm => temp.push(elm[filter.id]));

        // case insensitive sort (eg. eBay)
        return Array.from(new Set(temp)).sort((a, b) => {
            const nameA = a.toUpperCase();
            const nameB = b.toUpperCase();
            if (nameA < nameB) {
                return -1;
            }
            if (nameA > nameB) {
                return 1;
            }

            return 0;
        });
    };
    filters.forEach(f => {
        f.data = getFilterData(f);

        const control = document.getElementById(f.id)
        control.innerHTML += f.data.map(d => `<option value="${d}">${d}</option>`).join("\n")

        control.addEventListener("change", function() {
            f.value = control.value === "" ? null : control.value;
            reload();
        })

    });

    reload();
}
);

    </script>
